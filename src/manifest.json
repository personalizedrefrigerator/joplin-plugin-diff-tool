{
	"manifest_version": 1,
	"id": "io.github.personalizedrefrigerator.diff-view",
	"app_min_version": "3.0",
	"version": "0.1.1",
	"name": "Diff view",
	"description": "Shows differences between notes. This plugin can be used to resolve conflicts and compare related notes.",
	"author": "Henry Heino",
	"homepage_url": "https://github.com/personalizedrefrigerator/joplin-plugin-diff-tool",
	"repository_url": "https://github.com/personalizedrefrigerator/joplin-plugin-diff-tool",
	"keywords": ["codemirror", "diff", "compare", "conflict"],
	"categories": ["editor", "productivity"],
	"screenshots": [
		{
			"src": "images/screenshot.png",
			"label": "Screenshot: Shows a diff between parts of a file, with an 'accept' button."
		}
	],
	"icons": {
		"128": "images/icon-128.png",
		"256": "images/icon-256.png"
	},
	"platforms": ["mobile", "desktop"]
}
